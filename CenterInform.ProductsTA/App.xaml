<Application x:Class="CenterInform.ProductsTA.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:v="clr-namespace:CenterInform.ProductsTA.Views"
             xmlns:vm="clr-namespace:CenterInform.ProductsTA.ViewModels">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:CustomMessageBoxViewModel}">
            <v:CustomMessageBoxView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:DeleteDialogViewModel}">
            <v:DeleteDialogView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:RecurrentEntryDialogViewModel}">
            <v:RecurrentEntryDialogView/>
        </DataTemplate>
    </Application.Resources>
</Application>
